  // Test SistemVerifikasi
  tampilkan("Test 3: SistemVerifikasi")
  buat sistemVerifikasi = SistemVerifikasi()
  buat initVerif = sistemVerifikasi.inisialisasi_sistem()
  jika initVerif {
